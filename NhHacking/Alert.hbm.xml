<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="NhHacking"
                   assembly="NhHacking">
  <class name="Alert" table="alert">
    <id name="Id" type="int" unsaved-value="0" access="property">
      <generator class="identity" />
    </id>

    <property name="Name" column="name" />
    <map name="Messages" table="alert_message" cascade="all">
      <key column="alert_id" />
      <index column="channel" type="CxChannel" />
      <element column="message" type="System.String" />
    </map>
  </class>
</hibernate-mapping>